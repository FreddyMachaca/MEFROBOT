<Window x:Class="MEFRobot.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:MEFRobot"
        mc:Ignorable="d"
        Title="MEFRobot" Height="450" Width="800" Icon="Imagenes/wall-e-ok.png">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="auto"/>
            <RowDefinition Height="*"/>
        </Grid.RowDefinitions>
        <StackPanel Orientation="Horizontal" Grid.Row="0">
            <Label x:Name="lblEstado"/>
            <Label x:Name="lblBateria"/>
        </StackPanel>
        <Canvas x:Name="Terreno" Grid.Row="1"/>
    </Grid>
</Window>
